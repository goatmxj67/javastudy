package quiz06;

public class Bakery {

	// field
	private int breadCount;
	private int breadPrice;
	private int breadMoney;
	
	//constructor
	public Bakery(int breadCount, int breadPrice, int breadMoney) {
		this.breadCount = breadCount;
		this.breadPrice = breadPrice;
		this.breadMoney = breadMoney;
	}
	
	
	// method
	
	public void buy(Bakery bakery1, int customerBread) {
		breadCount -= customerBread;	
	}
	
	// 1. 판매 메소드
	
	// 2. info
	
	
	
	
	
	
}
